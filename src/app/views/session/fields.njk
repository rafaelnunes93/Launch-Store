

<div class="item">
    <div>Email</div>
    <div class="input">
        <input
        type="email"
        name="email"
        placeholder="Digite Seu Email"
        value="{{user.email}}"
        onblur="Validate.apply(this,'isEmail')"
        required
        >
    
    </div>
</div>


<div class="item">
    <div>Senha:</div>
    <div class="input">
        <input
        type="password"
        name="password"
        placeholder="Escolha uma senha segura"       
        >    
    </div>
</div>
